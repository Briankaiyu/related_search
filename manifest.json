{
	"manifest_version": 2,
	"name": "Related",
	"description": "Search for other content related to your current tab title.",
	"version": "1.5",
	"options_ui":{
		"page": "options.html",
		"chrome_style": true
	},
	"browser_action":{
		"default_icon": "icon.png",
		"default_title" : "Related"
	},
	"background":{
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions":[
		"storage",
		"tabs"
	]
	
}